#include <stdio.h>
int max(int mx,int min);
int main()
{
	int a,b,d;
	printf("enter first number:");
	scanf("%d",&a);
	printf("enter second number:");
	scanf("%d",&b);
	printf("maximum among %d and %d :",a,b);
	d=max(a,b);
	printf("%d",d);
	return 0;
}
int max(int mx,int min)
{
	if(mx>min)
	
	return mx;
	else 
	return min;	
}
